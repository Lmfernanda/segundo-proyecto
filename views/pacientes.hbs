<header>
        <h1>Centro médico Backend</h1>
    </header>
    <main class="aplicativos">
        <h2>Listado pacientes</h2>
        <table>
            <thead>
                <tr>
                    <th>Nombres</th>
                    <th>Apellidos</th>
                    <th>Número de cédula</th>
                    <th>Fecha de nacimiento</th>
                    <th>Telefono</th>
                </tr>
            </thead>
            <tbody id="cuerpo-tabla-pacientes">
                {{#each resultado}}
                <tr>
                    <td>{{nombres}}</td>
                    <td>{{apellidos}}</td>
                    <td>{{cedula}}</td>
                    <td>{{formatoFecha fecha_nacimiento}}</td>
                    <td>{{telefono}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        <a href="../index.html"><input type="button" value="Volver"></a>
    </main>
    <footer>
        <h2>REALIZADO POR:</h2>
        <p>Mar&iacute;a Fernanda L&oacute;pez</p>
    </footer>